<%- model_class = Prioridad -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:prioridad) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @prioridades.each do |prioridad| %>
      <tr>
        <td><%= link_to prioridad.id, prioridad_path(prioridad) %></td>
        <td><%= prioridad.prioridad %></td>
        <td><%=l prioridad.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      prioridad_path(prioridad), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_prioridad_path(prioridad), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      prioridad_path(prioridad),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_prioridad_path, :remote=>true,
            :class => 'btn btn-primary' %>

<div id="new-prioridad" style="display: none;"></div>